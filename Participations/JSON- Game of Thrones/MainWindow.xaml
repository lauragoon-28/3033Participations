<Window x:Class="JSON__Game_of_Thrones.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON__Game_of_Thrones"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/got.png"/>
        </Grid.Background>
        <Button x:Name="btnQuote" Content="Get Quote" HorizontalAlignment="Left" Margin="57,125,0,0" VerticalAlignment="Top" Height="46" Width="87" Click="btnQuote_Click"/>
        <Button x:Name="btnExport" Content="Export" HorizontalAlignment="Left" Margin="667,263,0,0" VerticalAlignment="Top" Height="46" Width="87" Click="btnExport_Click"/>
        <TextBox x:Name="txtQuote" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Width="440" IsEnabled="False" Height="184" FontSize="18"/>
        <Label x:Name="lblName" Content="" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontFamily="Bauhaus 93" FontSize="24" Height="110" Width="440"/>

    </Grid>
</Window>
